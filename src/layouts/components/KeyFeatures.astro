---
import { markdownify } from "@/lib/utils/textConverter";
import { Image } from "astro:assets";
import HomePageFeatures from "@/layouts/function-components/HomePageFeatures";
const { key_features } = Astro.props;
const { feature_list } = key_features;
---

<section class="section key-feature relative overflow-hidden">

  <div class="relative bg-transparent">
    <div class="container">
      <HomePageFeatures feature_list={feature_list} client:load />
    </div>
  </div>
</section>
